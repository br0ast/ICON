  "http://schema.org/citation" : [ {
    "@language" : "en",
    "@value" : "Sartini B. and Baroncini S. Icon Ontology. https://w3id.org/icon/ontology"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The ICON ontology deals with high granularity art interpretation. It was developed by conceptualizing Panofsky's theory of levels of interpretation, therefore artworks can be described according to Pre-iconographical, Iconographical and Iconological information."
  } ],
  "http://www.w3.org/2002/07/owl#backwardCompatibleWith" : [ {

        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>CompositionalStructure</IRI>
        <Literal xml:lang="en">The compositional structure identifies relevant structures that can be recognized in the layout of Artistic Motifs in a Composition, e.g. pyramidal, symmetrical.</Literal>
    </AnnotationAssertion>

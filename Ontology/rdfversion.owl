<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#"
     xml:base="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:crm="http://erlangen-crm.org/current/"
     xmlns:dct="https://www.w3.org/1999/xhtml/vocab#dct:"
     xmlns:dul="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:icon="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:vocab="https://www.w3.org/1999/xhtml/vocab#"
     xmlns:schema="http://schema.org/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol">
        <owl:imports rdf:resource="http://erlangen-crm.org/current/"/>
        <owl:imports rdf:resource="http://purl.org/emmedi/hico/2020-03-31"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"/>
        <owl:imports rdf:resource="https://www.w3id.org/simulation/ontology/"/>
        <dc:contributor xml:lang="en">Aldo Gangemi</dc:contributor>
        <dc:contributor xml:lang="en">Marieke van Erp</dc:contributor>
        <dc:creator>Bruno Sartini</dc:creator>
        <dc:creator>Sofia Baroncini</dc:creator>
        <dcterms:created xml:lang="en">April 12th, 2022</dcterms:created>
        <dcterms:issued xml:lang="en">April 12th, 2022</dcterms:issued>
        <dcterms:license xml:lang="en">https://creativecommons.org/licenses/by/4.0/</dcterms:license>
        <dcterms:publisher xml:lang="en">Bruno Sartini</dcterms:publisher>
        <dcterms:title xml:lang="en">ICON Ontology</dcterms:title>
        <vann:preferredNamespacePrefix xml:lang="en">icon</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri xml:lang="en">tbd</vann:preferredNamespaceUri>
        <schema:citation xml:lang="en">tbd</schema:citation>
        <rdfs:comment xml:lang="en">The ICON ontology deals with high granularity art interpretation. It was developed by conceptualizing Panofsky&apos;s theory of levels of interpretation, therefore artworks can be described according to Pre-iconographical, Iconographical and Iconological information.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/created -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/issued -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/issued"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#citation -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2001/XMLSchema#citation"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart">
        <owl:equivalentProperty rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPart"/>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf">
        <owl:equivalentProperty rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#partOf"/>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#aboutWorkOfArt -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#aboutWorkOfArt">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#interpretedByRecognition"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Recognition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Artwork"/>
        <rdfs:comment xml:lang="en">This relation associates each Recognition directly to the artwork. In this way, it is possible to retrieve easily all the interpretations about an artwork. Therefore, it a highly recommended good practice to declare it for each interpretation.</rdfs:comment>
        <rdfs:label xml:lang="en">About Work Of Art</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#associatedForm -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#associatedForm">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P140_assigned_attribute_to"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isPureFormOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#PreiconographicalRecognition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#PureForm"/>
        <rdfs:comment>This relation associates a Pure Form to the Pre-iconographical Recognition that describes it at the first level of interpretation.</rdfs:comment>
        <rdfs:label xml:lang="en">Associated Form</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#composedOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#composedOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#takesPartIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Invenzione"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Character"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Event"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#NamedObject"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Personification"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Place"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Symbol"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This Property links an invenzione with its components, which may vary from Characters, Events, Named Objects, Places to Personifications, Symbols according to the kind of Invenzione is being described.</rdfs:comment>
        <rdfs:label xml:lang="en">Composed Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasArtisticMotif -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasArtisticMotif">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isArtisticMotifOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IntrinsicMeaning"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ArtisticMotif"/>
        <rdfs:comment xml:lang="en">This relation links an Intrinsic Meaning to the Artistic Motif recognized at the first level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Artistic Motif, and not only to the general artwork.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Artistic Motif</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCharacter -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCharacter">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isCharacterOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Image"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Character"/>
        <rdfs:comment xml:lang="en">This relation links an image to the Character depicted</rdfs:comment>
        <rdfs:label xml:lang="en">Has Character</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasComposition -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasComposition">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isCompositionOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IntrinsicMeaning"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Composition"/>
        <rdfs:comment xml:lang="en">This relation links an Intrinsic Meaning to the Composition recognized at the first level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Composition, and not only to the general artwork.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Composition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCompositionalStructure -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCompositionalStructure">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isCompositionalStructureOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Composition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#CompositionalStructure"/>
        <rdfs:comment>This relation describes the arrangement of the Artistic Motifs composing the Composition. It is suggested to use it only when they are arranged in a relevant observable structure (e.g. pyramidal, oblique)</rdfs:comment>
        <rdfs:label xml:lang="en">Has Compositional Structure</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCopiedMotif -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCopiedMotif">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isCopiedMotifOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#FormalMotifRecognition"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ArtisticMotif"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This relation associates to the act of Formal Motif Recognition the Artistic Motif representing the copied visual pattern. To be used when it is affirmed that a specific feature has as a prototype the same feature of a specific visual item.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Copied Motif</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasEvent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasEvent">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isEventOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Image"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Event"/>
        <rdfs:comment xml:lang="en">This relation links an image to the Event depicted</rdfs:comment>
        <rdfs:label xml:lang="en">Has Event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasExpressionalMeaning -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasExpressionalMeaning">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isExpressionalMeaningOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ArtisticMotif"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ExpressionalQuality"/>
        <rdfs:comment>This relation links the Expressional Meaning recognized to the Artistic Motif</rdfs:comment>
        <rdfs:label xml:lang="en">Has Expressional Meaning</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasFactualMeaning -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasFactualMeaning">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isFactualMeaningOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ArtisticMotif"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Action"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#NaturalElement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>This relation links the Factual Meaning recognized (Actions or Natural Elements) to the Artistic Motif</rdfs:comment>
        <rdfs:label xml:lang="en">Has Factual Meaning</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasImage -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasImage">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isImageOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IntrinsicMeaning"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Image"/>
        <rdfs:comment xml:lang="en">This relation links an Intrinsic Meaning to the Image recognized at the second level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Image, and not only to the general artwork.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Image</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasInvenzione -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasInvenzione">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isInvenzioneOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IntrinsicMeaning"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Invenzione"/>
        <rdfs:comment xml:lang="en">This relation links an Intrinsic Meaning to the Invenzione recognized at the second level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this invenzione, and not only to the general artwork.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Invenzione</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasNamedObject -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasNamedObject">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isNamedObjectOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Image"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#NamedObject"/>
        <rdfs:comment xml:lang="en">This relation links an image to the Named Object depicted</rdfs:comment>
        <rdfs:label xml:lang="en">Has Named Object</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPart">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P106_is_composed_of"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#partOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Composition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ArtisticMotif"/>
        <rdfs:comment xml:lang="en">This relation links a Composition to the Artistic Motifs of which it consists.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPersonification -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPersonification">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isPersonificationOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Image"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Personification"/>
        <rdfs:comment xml:lang="en">This relation links an image to the Personification depicted</rdfs:comment>
        <rdfs:label xml:lang="en">Has Personification</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPlace -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPlace">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isPlaceOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Image"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Place"/>
        <rdfs:comment xml:lang="en">This relation links an image to the Place depicted</rdfs:comment>
        <rdfs:label xml:lang="en">Has Place</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPrototypicalMotif -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPrototypicalMotif">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isPrototypicalMotifOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#FormalMotifRecognition"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ArtisticMotif"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This relation associates to the act of the Recognition of a formal motif to the artistic motif from which the visual pattern derives. It should be used when it is affirmed that a specific feature has as a prototype the same feature of a specific visual item.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Prototypical Motif</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasRecAttribute -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasRecAttribute">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isRecAttributeOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Image"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ArtisticMotif"/>
        <rdfs:comment xml:lang="en">This relation identifies objects belonging to the level 1 as fundamental for the recognition of a certain subject. It is suggested that the Artistic Motif of the object is included in the Composition corresponding to the subject.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Rec Attribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasSymbol -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasSymbol">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P138_represents"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isSymbolOf"/>

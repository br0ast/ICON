        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IconographicalRecognition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Image"/>
        <rdfs:comment xml:lang="en">This relation associates the Image to the Iconographical Recognition that recognizes it at the second level of interpretation.</rdfs:comment>
        <rdfs:label xml:lang="en">Recognized Image</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedIntrinsicMeanig -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedIntrinsicMeanig">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IconologicalRecognition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IntrinsicMeaning"/>
        <rdfs:comment xml:lang="en">This relation associates the Intrinsic Meaning to the Iconological Recognition that recognizes it at the third level of interpretation.</rdfs:comment>
        <rdfs:label xml:lang="en">Recognized Intrinsic Meanig</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedInvenzione -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedInvenzione">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P141_assigned"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IconographicalRecognition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Invenzione"/>
        <rdfs:comment xml:lang="en">This relation associates the Invenzione to the Iconographical Recognition that recognizes it at the second level of interpretation.</rdfs:comment>
        <rdfs:label xml:lang="en">Recognized Invenzione</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#refersToArtisticMotif -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#refersToArtisticMotif">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P140_assigned_attribute_to"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#IconographicalRecognition"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#ArtisticMotif"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This relation associates a Composition or an Artistic Motif to the Iconographical Recognition that describes it at the second level of interpretation.</rdfs:comment>
        <rdfs:label xml:lang="en">Refers To Artistic Motif</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#takesPartIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#takesPartIn">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Character"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Event"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#NamedObject"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Personification"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Place"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Symbol"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#Invenzione"/>
        <rdfs:comment xml:lang="en">Inverse property of composed of</rdfs:comment>
        <rdfs:label xml:lang="en">Takes part in</rdfs:label>
    </owl:ObjectProperty>
    



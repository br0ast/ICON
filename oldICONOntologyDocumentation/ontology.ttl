@prefix : <http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#> .

<http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol> rdf:type owl:Ontology ;
                                                                 owl:imports <http://erlangen-crm.org/current/> ,
                                                                             <http://purl.org/emmedi/hico/2020-03-31> ,
                                                                             <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ,
                                                                             <https://www.w3id.org/simulation/ontology/> ;
                                                                 <http://purl.org/dc/elements/1.1/contributor> "Aldo Gangemi"@en ,
                                                                                                               "Marieke van Erp"@en ;
                                                                 <http://purl.org/dc/elements/1.1/creator> "Bruno Sartini" ,
                                                                                                           "Sofia Baroncini" ;
                                                                 <http://purl.org/dc/terms/issued> "April 12th, 2022"@en ;
                                                                 <http://purl.org/dc/terms/license> "https://creativecommons.org/licenses/by/4.0/"@en ;
                                                                 <http://purl.org/dc/terms/title> "ICON Ontology"@en ;
                                                                 <http://purl.org/vocab/vann/preferredNamespacePrefix> "icon"@en ;
                                                                 <http://purl.org/vocab/vann/preferredNamespaceUri> "tbd"@en ;
                                                                 xsd:citation "Waitingforthepaper"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
<http://purl.org/dc/terms/issued> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2001/XMLSchema#citation
xsd:citation rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> rdfs:domain :ArtisticMotif .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#aboutWorkOfArt
:aboutWorkOfArt rdf:type owl:ObjectProperty ;
                owl:inverseOf :interpretedByRecognition ;
                rdfs:domain :Recognition ;
                rdfs:range :Artwork ;
                rdfs:comment "This relation associates each Recognition directly to the artwork. In this way, it is possible to retrieve easily all the interpretations about an artwork. Therefore, it a highly recommended good practice to declare it for each interpretation."@en ;
                rdfs:label "About Work Of Art"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#associatedForm
:associatedForm rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://erlangen-crm.org/current/P140_assigned_attribute_to> ;
                owl:inverseOf :isPureFormOf ;
                rdfs:domain :PreiconographicalRecognition ;
                rdfs:range :PureForm ;
                rdfs:comment "This relation associates a Pure Form to the Pre-iconographical Recognition that describes it at the first level of interpretation." ;
                rdfs:label "Associated Form"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#composedOf
:composedOf rdf:type owl:ObjectProperty ;
            rdfs:domain :Invenzione ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Character
                                       :Event
                                       :NamedObject
                                       :Personification
                                       :Place
                                       :Symbol
                                     )
                       ] ;
            rdfs:comment "This Property links an invenzione with its components, which may vary from Characters, Events, Named Objects, Places to Personifications, Symbols according to the kind of Invenzione is being described."@en ;
            rdfs:label "Composed Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasArtisticMotif
:hasArtisticMotif rdf:type owl:ObjectProperty ;
                  rdfs:domain :IntrinsicMeaning ;
                  rdfs:range :ArtisticMotif ;
                  rdfs:comment "This relation links an Intrinsic Meaning to the Artistic Motif recognized at the first level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Artistic Motif, and not only to the general artwork."@en ;
                  rdfs:label "Has Artistic Motif"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCharacter
:hasCharacter rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://erlangen-crm.org/current/P138_represents> ;
              owl:inverseOf :isCharacterOf ;
              rdfs:domain :Image ;
              rdfs:range :Character ;
              rdfs:comment "This relation links an image to the Character depicted"@en ;
              rdfs:label "Has Character"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasComposition
:hasComposition rdf:type owl:ObjectProperty ;
                rdfs:domain :IntrinsicMeaning ;
                rdfs:range :Composition ;
                rdfs:comment "This relation links an Intrinsic Meaning to the Composition recognized at the first level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Composition, and not only to the general artwork."@en ;
                rdfs:label "Has Composition"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCompositionalStructure
:hasCompositionalStructure rdf:type owl:ObjectProperty ;
                           owl:inverseOf :isCompositionalStructureOf ;
                           rdfs:domain :Composition ;
                           rdfs:range :CompositionalStructure ;
                           rdfs:comment "This relation describes the arrangement of the Artistic Motifs composing the Composition. It is suggested to use it only when they are arranged in a relevant observable structure (e.g. pyramidal, oblique)" ;
                           rdfs:label "Has Compositional Structure"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasCopiedMotif
:hasCopiedMotif rdf:type owl:ObjectProperty ;
                owl:inverseOf :isCopiedMotifOf ;
                rdfs:domain [ rdf:type owl:Restriction ;
                              owl:onProperty :hasPrototypicalMotif ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :FormalMotifRecognition
                            ] ;
                rdfs:range [ rdf:type owl:Restriction ;
                             owl:onProperty :hasPrototypicalMotif ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass [ rdf:type owl:Class ;
                                           owl:unionOf ( :ArtisticMotif
                                                         :Composition
                                                       )
                                         ]
                           ] ;
                rdfs:comment "This relation associates to the act of Formal Motif Recognition the Artistic Motif representing the copied visual pattern. To be used when it is affirmed that a specific feature has as a prototype the same feature of a specific visual item."@en ;
                rdfs:label "Has Copied Motif"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://erlangen-crm.org/current/P138_represents> ;
          owl:inverseOf :isEventOf ;
          rdfs:domain :Image ;
          rdfs:range :Event ;
          rdfs:comment "This relation links an image to the Event depicted"@en ;
          rdfs:label "Has Event"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasExpressionalMeaning
:hasExpressionalMeaning rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://erlangen-crm.org/current/P138_represents> ;
                        owl:inverseOf :isExpressionalMeaningOf ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :ArtisticMotif
                                                    :Composition
                                                  )
                                    ] ;
                        rdfs:range :ExpressionalQuality ;
                        rdfs:comment "This relation links the Expressional Meaning recognized to the Artistic Motif" ;
                        rdfs:label "Has Expressional Meaning"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasFactualMeaning
:hasFactualMeaning rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://erlangen-crm.org/current/P138_represents> ;
                   owl:inverseOf :isFactualMeaningOf ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :ArtisticMotif
                                               :Composition
                                             )
                               ] ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( :Action
                                              :NaturalElement
                                            )
                              ] ;
                   rdfs:comment "This relation links the Factual Meaning recognized (Actions or Natural Elements) to the Artistic Motif" ;
                   rdfs:label "Has Factual Meaning"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasImage
:hasImage rdf:type owl:ObjectProperty ;
          rdfs:domain :IntrinsicMeaning ;
          rdfs:range :Image ;
          rdfs:comment "This relation links an Intrinsic Meaning to the Image recognized at the second level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this Image, and not only to the general artwork."@en ;
          rdfs:label "Has Image"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasInvenzione
:hasInvenzione rdf:type owl:ObjectProperty ;
               owl:inverseOf :isInvenzioneOf ;
               rdfs:domain :IntrinsicMeaning ;
               rdfs:range :Invenzione ;
               rdfs:comment "This relation links an Intrinsic Meaning to the Invenzione recognized at the second level of interpretation. It should be used to express that the third level meaning associated to the Intrinsic Meaning refers specifically to this invenzione, and not only to the general artwork."@en ;
               rdfs:label "Has Invenzione"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasNamedObject
:hasNamedObject rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://erlangen-crm.org/current/P138_represents> ;
                owl:inverseOf :isNamedObjectOf ;
                rdfs:domain :Image ;
                rdfs:range :NamedObject ;
                rdfs:comment "This relation links an image to the Named Object depicted"@en ;
                rdfs:label "Has Named Object"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPart
:hasPart rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://erlangen-crm.org/current/P106_is_composed_of> ;
         owl:inverseOf :partOf ;
         rdfs:domain :Composition ;
         rdfs:range :ArtisticMotif ;
         rdfs:comment "This relation links a Composition to the Artistic Motifs of which it consists."@en ;
         rdfs:label "Has Part"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPersonification
:hasPersonification rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://erlangen-crm.org/current/P138_represents> ;
                    owl:inverseOf :isPersonificationOf ;
                    rdfs:domain :Image ;
                    rdfs:range :Personification ;
                    rdfs:comment "This relation links an image to the Personification depicted"@en ;
                    rdfs:label "Has Personification"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPlace
:hasPlace rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://erlangen-crm.org/current/P138_represents> ;
          owl:inverseOf :isPlaceOf ;
          rdfs:domain :Image ;
          rdfs:range :Place ;
          rdfs:comment "This relation links an image to the Place depicted"@en ;
          rdfs:label "Has Place"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasPrototypicalMotif
:hasPrototypicalMotif rdf:type owl:ObjectProperty ;
                      owl:inverseOf :isPrototypicalMotifOf ;
                      rdfs:domain [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasPrototypicalMotif ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :FormalMotifRecognition
                                  ] ;
                      rdfs:range [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasPrototypicalMotif ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass [ rdf:type owl:Class ;
                                                 owl:unionOf ( :ArtisticMotif
                                                               :Composition
                                                             )
                                               ]
                                 ] ;
                      rdfs:comment "This relation associates to the act of Formal Motif Recognition the Artistic Motif from which the visual pattern derives. It should be used when it is affirmed that a specific feature has as a prototype the same feature of a specific visual item."@en ;
                      rdfs:label "Has Prototypical Motif"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasRecAttribute
:hasRecAttribute rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isRecAttributeOf ;
                 rdfs:domain :Image ;
                 rdfs:range :ArtisticMotif ;
                 rdfs:comment "This relation identifies objects belonging to the level 1 as fundamental for the recognition of a certain subject. It is suggested that the Artistic Motif of the object is included in the Composition corresponding to the subject."@en ;
                 rdfs:label "Has Rec Attribute"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#hasSymbol
:hasSymbol rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://erlangen-crm.org/current/P138_represents> ;
           owl:inverseOf :isCharacterOf ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Image
                                       :IntrinsicMeaning
                                     )
                       ] ;
           rdfs:range :Symbol ,
                      <https://www.w3id.org/simulation/ontology/Simulation> ;
           rdfs:comment "This relation links an image to the Symbol depicted"@en ;
           rdfs:label "Has Symbol"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#interpretedByRecognition
:interpretedByRecognition rdf:type owl:ObjectProperty ;
                          rdfs:comment "Inverse property of AboutWorkOfArt"@en ;
                          rdfs:label "Interpreted By Recognition"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#interpretedByRecogniton
:interpretedByRecogniton rdf:type owl:ObjectProperty ;
                         rdfs:comment "Inverse property of aboutWorkOfArt"@en ;
                         rdfs:label "Interpreted By Recogniton"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isCharacterOf
:isCharacterOf rdf:type owl:ObjectProperty ;
               rdfs:comment "Inverse property of hasCharacter"@en ,
                            "Inverse property of hasSymbol"@en ;
               rdfs:label "Is Character Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isCompositionalStructureOf
:isCompositionalStructureOf rdf:type owl:ObjectProperty ;
                            rdfs:comment "Inverse property of hasCompositionalStructure"@en ;
                            rdfs:label "Is Compositional Structure Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isConceptualObjectOf
:isConceptualObjectOf rdf:type owl:ObjectProperty ;
                      owl:inverseOf :recognizedConceptualObject ;
                      rdfs:comment "Inverse property of recognizedConceptualObject"@en ;
                      rdfs:label "Is Conceptual Object Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isCopiedMotifOf
:isCopiedMotifOf rdf:type owl:ObjectProperty ;
                 rdfs:comment "Inverse property of hasCopiedMotif"@en ;
                 rdfs:label "Is Copied Motif Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isCulturalPhenomenonOf
:isCulturalPhenomenonOf rdf:type owl:ObjectProperty ;
                        owl:inverseOf :recognizedCulturalPhenomenon ;
                        rdfs:comment "Inverse property of recognizedCulturalPhenomenon"@en ;
                        rdfs:label "Is Cultural Phenomenon Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isEventOf
:isEventOf rdf:type owl:ObjectProperty ;
           rdfs:comment "Inverse property of hasEvent"@en ;
           rdfs:label "Is Event Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isExpressionalMeaningOf
:isExpressionalMeaningOf rdf:type owl:ObjectProperty ;
                         rdfs:comment "Inverse property of hasExpressionalMeaning"@en ;
                         rdfs:label "Is Expressional Meaning Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isFactualMeaningOf
:isFactualMeaningOf rdf:type owl:ObjectProperty ;
                    rdfs:comment "Inverse property of hasFactualMeaning"@en ;
                    rdfs:label "Is Factual Meaning Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isIconographicallyRecognizedBy
:isIconographicallyRecognizedBy rdf:type owl:ObjectProperty ;
                                owl:inverseOf :recognizedImage ;
                                rdfs:comment "Inverse property of recognizedImage"@en ;
                                rdfs:label "Is Iconographically Recognized By"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isInvenzioneOf
:isInvenzioneOf rdf:type owl:ObjectProperty ;
                rdfs:comment "Inverse property of hasInvenzione"@en ;
                rdfs:label "Is Invenzione Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isListedIn
:isListedIn rdf:type owl:ObjectProperty ;
            rdfs:domain :Recognition ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :IconographicalDescription
                                       :IconographicalSynthesis
                                       :PreiconographicalDescription
                                     )
                       ] ;
            rdfs:label "Is Listed In"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isNamedObjectOf
:isNamedObjectOf rdf:type owl:ObjectProperty ;
                 rdfs:comment "Inverse property of hasNamedObject"@en ;
                 rdfs:label "Is Named Object Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isPersonificationOf
:isPersonificationOf rdf:type owl:ObjectProperty ;
                     rdfs:comment "Inverse property of hasPersonification"@en ;
                     rdfs:label "Is Personification Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isPlaceOf
:isPlaceOf rdf:type owl:ObjectProperty ;
           rdfs:comment "Inverse property of hasPlace"@en ;
           rdfs:label "Is Place Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isPrototypicalMotifOf
:isPrototypicalMotifOf rdf:type owl:ObjectProperty ;
                       rdfs:comment "Inverse property of hasPrototypicalMotif"@en ;
                       rdfs:label "Is Prototypical Motif Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isPureFormOf
:isPureFormOf rdf:type owl:ObjectProperty ;
              rdfs:comment "Inverse property of associatedForm"@en ;
              rdfs:label "Is Pure Form Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isRecAttributeOf
:isRecAttributeOf rdf:type owl:ObjectProperty ;
                  rdfs:comment "Inverse property of hasRecAttribute"@en ;
                  rdfs:label "Is Rec Attribute Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isRecognOf
:isRecognOf rdf:type owl:ObjectProperty ;
            owl:inverseOf :recognizedInvenzione ;
            rdfs:comment "Inverse property of recognizedInvenzione"@en ;
            rdfs:label "Is Recogn Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isRecognizedArtisticMotifOf
:isRecognizedArtisticMotifOf rdf:type owl:ObjectProperty ;
                             owl:inverseOf :recognizedArtisticMotif ;
                             rdfs:comment "Inverse property of recognizedArtisticMotif"@en ;
                             rdfs:label "Is Recognized Artistic Motif Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#isRecognizedCompositionOf
:isRecognizedCompositionOf rdf:type owl:ObjectProperty ;
                           owl:inverseOf :recognizedComposition ;
                           rdfs:comment "Inverse property of recognizedComposition"@en ;
                           rdfs:label "Is Recognized Composition Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:comment "Inverse property of hasPart"@en ;
        rdfs:label "Part Of"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedArtisticMotif
:recognizedArtisticMotif rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <http://erlangen-crm.org/current/P141_assigned> ;
                         rdfs:domain :PreiconographicalRecognition ;
                         rdfs:range :ArtisticMotif ;
                         rdfs:comment "This relation associates the Artistic Motif to the Pre-iconographical Recognition that recognizes it at the first level of interpretation."@en ;
                         rdfs:label "Recognized Artistic Motif"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedComposition
:recognizedComposition rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://erlangen-crm.org/current/P141_assigned> ;
                       rdfs:domain :PreiconographicalRecognition ;
                       rdfs:range :Composition ;
                       rdfs:comment "This relation associates the Composition to the Pre-iconographical Recognition that recognizes it at the first level of interpretation."@en ;
                       rdfs:label "Recognized Composition"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedConceptualObject
:recognizedConceptualObject rdf:type owl:ObjectProperty ;
                            rdfs:domain :IntrinsicMeaning ;
                            rdfs:range <http://erlangen-crm.org/current/E28_Conceptual_Object> ;
                            rdfs:comment "This relation expresses the Concept that is related to the Intrinsic Meaning"@en ;
                            rdfs:label "Recognized Conceptual Object"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedCulturalPhenomenon
:recognizedCulturalPhenomenon rdf:type owl:ObjectProperty ;
                              rdfs:domain :IntrinsicMeaning ;
                              rdfs:range :CulturalPhenomenon ;
                              rdfs:comment "This relation expresses the Cultural Phenomenon that is related to the Intrinsic Meaning"@en ;
                              rdfs:label "Recognized Cultural Phenomenon"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedGenre
:recognizedGenre rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://erlangen-crm.org/current/P141_assigned> ;
                 rdfs:domain :Recognition ;
                 rdfs:range :ArtGenre ;
                 rdfs:label "Recognized Genre"@en ;
                 owl:deprecated "since new version 25/03"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedImage
:recognizedImage rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://erlangen-crm.org/current/P141_assigned> ;
                 rdfs:domain :IconographicalRecognition ;
                 rdfs:range :Image ;
                 rdfs:comment "This relation associates the Image to the Iconographical Recognition that recognizes it at the second level of interpretation."@en ;
                 rdfs:label "Recognized Image"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedIntrinsicMeanig
:recognizedIntrinsicMeanig rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:domain :IconologicalRecognition ;
                           rdfs:range :IntrinsicMeaning ;
                           rdfs:comment "This relation associates the Intrinsic Meaning to the Iconological Recognition that recognizes it at the third level of interpretation."@en ;
                           rdfs:label "Recognized Intrinsic Meanig"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#recognizedInvenzione
:recognizedInvenzione rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://erlangen-crm.org/current/P141_assigned> ;
                      rdfs:domain :IconographicalRecognition ;
                      rdfs:range :Invenzione ;
                      rdfs:comment "This relation associates the Invenzione to the Iconographical Recognition that recognizes it at the second level of interpretation."@en ;
                      rdfs:label "Recognized Invenzione"@en .


###  http://www.semanticweb.org/bruno/ontologies/2021/4/iconsymbol#refersToArtisticMotif
:refersToArtisticMotif rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://erlangen-crm.org/current/P140_assigned_attribute_to> ;
                       rdfs:domain :IconographicalRecognition ;
                       rdfs:range [ rdf:type owl:Class ;
                                    owl:unionOf ( :ArtisticMotif
                                                  :Composition
                                                )
                                  ] ;
                       rdfs:comment "This relation associates a Composition or an Artistic Motif to the Iconographical Recognition that describes it at the second level of interpretation."@en ;
                       rdfs:label "Refers To Artistic Motif"@en .


#################################################################
#    Data properties
#################################################################

###  https://www.w3.org/ns/prov-o-inverses#regionDescription
